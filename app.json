{
  "name": "Rails Base Template",
  "description": "A Rails template that includes everything you need to get started with Rails",
  "keywords": [
    "productivity",
    "rubyonrails",
    "ruby",
    "HTML5"
  ],
  "website": "https://small-sharp-tool.com/",
  "repository": "https://github.com/vshousehq/rails-base",
  "logo": "https://upload.wikimedia.org/wikipedia/commons/6/62/Ruby_On_Rails_Logo.svg",
  "success_url": "/welcome",
  "scripts": {
    "postdeploy": "bundle exec rake bootstrap"
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "standard-1x"
    }
  },
  "image": "heroku/ruby",
  "addons": [
    "heroku-redis",
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "13"
      }
    }
  ]
}